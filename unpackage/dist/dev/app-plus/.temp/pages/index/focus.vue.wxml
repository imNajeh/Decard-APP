<import src="/components/uniDrawer.vue.wxml" />
<template name="404a58be">
  <view class="_view data-v-3dde7658 content">
    <view class="_view data-v-3dde7658 focus_wrap">
      <template mode="right" bindclose="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}" data="{{...$root[$kk+'0'], $root}}" is="b35ebbc0"></template>
      <view class="_view data-v-3dde7658 focus_card" style=" {{('background-color:' + color + ';')}}">
        <view class="_view data-v-3dde7658 title">{{name}}</view>
        <view class="_view data-v-3dde7658 circle dot" wx:if="{{!complete}}" style=" {{('background-color:' + color + ';')}}">
          <swiper bindchange="handleProxy" circular="{{true}}" current-item-id="{{current}}" class="_swiper data-v-3dde7658 music" duration="{{800}}" data-eventid="{{'1'}}" data-comkey="{{$k}}">
            <swiper-item class="_swiper-item data-v-3dde7658 music_inner" wx:key="item" key="{{item}}" item-id="{{index}}" wx:for="{{itemList}}" wx:for-index="index" wx:for-item="item">
              <text class="_text data-v-3dde7658">{{item.text}}</text>
            </swiper-item>
          </swiper>
        </view>
        <view class="_view data-v-3dde7658 circle dot" wx:else style=" {{('background-color:' + color + ';')}}"> 已完成专注 </view>
        <view class="_view data-v-3dde7658 counter" wx:if="{{!complete}}"> + {{formatSecond?formatSecond:'00:00:00'}} </view>
        <view class="_view data-v-3dde7658 counter" wx:else> 奖励币已存入您账户 </view>
        <view class="_view data-v-3dde7658 btn_wrap">
          <view class="_view data-v-3dde7658 btn" wx:if="{{!complete}}" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}">{{is_pause?'继续':'暂停'}}</view>
          <view class="_view data-v-3dde7658 btn" wx:if="{{!complete}}" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}">停止</view>
          <view class="_view data-v-3dde7658 btn" wx:if="{{complete}}" bindtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}">返回首页</view>
        </view>
      </view>
    </view>
  </view>
</template>