<template name="db6798f6">
  <view class="_view data-v-53c73a9c content">
    <view class="_view data-v-53c73a9c timing_wrap">
      <view class="_view data-v-53c73a9c top">
        <view class="_view data-v-53c73a9c timing_card">
          <view class="_view data-v-53c73a9c card_time">{{total}} h</view>
          <image class="_image data-v-53c73a9c timing_card_icon" src="{{'../../static/card_icon/'+icon+'.png'}}" mode="scaleToFill"></image>
          <view class="_view data-v-53c73a9c card_name">{{name}}</view>
        </view>
        <view class="_view data-v-53c73a9c add_time">
          <text class="_text data-v-53c73a9c">+ {{time[0]}}</text>
          <text class="_text data-v-53c73a9c equal">= {{time[1]}}</text>
        </view>
      </view>
      <view class="_view data-v-53c73a9c add">
        <text class="_text data-v-53c73a9c">添加专注时间：</text>
        <text class="_text data-v-53c73a9c add_sub">m 代表分钟，h 代表小时</text>
        <view class="_view data-v-53c73a9c add_items">
          <view class="_view data-v-53c73a9c item" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}">1 m</view>
          <view class="_view data-v-53c73a9c item" bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}">5 m</view>
          <view class="_view data-v-53c73a9c item" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}">10 m</view>
        </view>
        <view class="_view data-v-53c73a9c add_items">
          <view class="_view data-v-53c73a9c item" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}">25 m</view>
          <view class="_view data-v-53c73a9c item" bindtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}">50 m</view>
          <view class="_view data-v-53c73a9c item" bindtap="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}">1 h</view>
        </view>
        <view class="_view data-v-53c73a9c add_items">
          <view class="_view data-v-53c73a9c item zero" bindtap="handleProxy" data-eventid="{{'6'}}" data-comkey="{{$k}}">清零</view>
        </view>
        <view class="_view data-v-53c73a9c add_time_btn">开始专注</view>
      </view>
    </view>
  </view>
</template>