<template name="e737085e">
  <view class="_view data-v-9c269e04 content">
    <view class="_view data-v-9c269e04 timing_wrap">
      <view class="_view data-v-9c269e04 top">
        <view class="_view data-v-9c269e04 timing_card">
          <view class="_view data-v-9c269e04 card_time">{{total}} h</view>
          <image class="_image data-v-9c269e04 timing_card_icon" src="{{'../../static/card_icon/'+icon+'.png'}}" mode="scaleToFill"></image>
          <view class="_view data-v-9c269e04 card_name">{{name}}</view>
        </view>
        <view class="_view data-v-9c269e04 add_time">
          <text class="_text data-v-9c269e04">+ {{time[0]}}</text>
          <text class="_text data-v-9c269e04 equal">= {{time[1]}}</text>
        </view>
      </view>
      <view class="_view data-v-9c269e04 add">
        <text class="_text data-v-9c269e04">添加专注时间：</text>
        <text class="_text data-v-9c269e04 add_sub">m 代表分钟，h 代表小时</text>
        <view class="_view data-v-9c269e04 add_items">
          <view class="_view data-v-9c269e04 item" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}">1 m</view>
          <view class="_view data-v-9c269e04 item" bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}">5 m</view>
          <view class="_view data-v-9c269e04 item" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}">10 m</view>
        </view>
        <view class="_view data-v-9c269e04 add_items">
          <view class="_view data-v-9c269e04 item" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}">25 m</view>
          <view class="_view data-v-9c269e04 item" bindtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}">50 m</view>
          <view class="_view data-v-9c269e04 item" bindtap="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}">1 h</view>
        </view>
        <view class="_view data-v-9c269e04 add_items">
          <view class="_view data-v-9c269e04 item zero" bindtap="handleProxy" data-eventid="{{'6'}}" data-comkey="{{$k}}">清零</view>
        </view>
        <view class="_view data-v-9c269e04 add_time_btn" bindtap="handleProxy" data-eventid="{{'7'}}" data-comkey="{{$k}}">开始专注</view>
      </view>
    </view>
  </view>
</template>