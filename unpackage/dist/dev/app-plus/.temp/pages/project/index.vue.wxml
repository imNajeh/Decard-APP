<template name="7dc0e53c">
  <view class="_view data-v-9421a5d6 content">
    <view class="_view data-v-9421a5d6 project_wrap">
      <view class="_view data-v-9421a5d6 project_classify_item" wx:key="index" key="{{index}}" wx:for="{{lists}}" wx:for-index="index" wx:for-item="list">
        <view class="_view data-v-9421a5d6 project_classify_item_top">
          <view class="_view data-v-9421a5d6 title">
            <text class="_text data-v-9421a5d6">{{list.classify_name}}</text>
            <text class="_text data-v-9421a5d6 num">共 {{list.list_item.length}} 个项目</text>
          </view>
          <view class="_view data-v-9421a5d6 total">
            <image class="_image data-v-9421a5d6 timer" src="../../static/icon/time.png" mode="aspectFill"></image> {{list.total}} h </view>
        </view>
        <view class="_view data-v-9421a5d6 project_item" wx:key="itemIndex" key="{{itemIndex}}" wx:for="{{list.list_item}}" wx:for-index="itemIndex" wx:for-item="item">
          <view class="_view data-v-9421a5d6 card" style=" {{('background-color:' + item.color + ';')}}">
            <image class="_image data-v-9421a5d6 icon" src="{{'../../static/card_icon/'+item.icon+'.png'}}" mode="scaleToFill"></image>
          </view>
          <view class="_view data-v-9421a5d6 title">
            <text class="_text data-v-9421a5d6">{{item.name}}</text>
            <text class="_text data-v-9421a5d6 second">最近更新：{{item.date}}</text>
          </view>
          <view class="_view data-v-9421a5d6 time">{{item.time}} h</view>
        </view>
      </view>
    </view>
  </view>
</template>