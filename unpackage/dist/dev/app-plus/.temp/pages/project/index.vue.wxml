<template name="782ecc2e">
  <view class="_view data-v-17b5b1e1 content">
    <view class="_view data-v-17b5b1e1 project_wrap">
      <view class="_view data-v-17b5b1e1 project_classify_item" wx:key="index" key="{{index}}" wx:for="{{lists}}" wx:for-index="index" wx:for-item="list">
        <view class="_view data-v-17b5b1e1 project_classify_item_top">
          <view class="_view data-v-17b5b1e1 title">
            <text class="_text data-v-17b5b1e1">{{list.classify_name}}</text>
            <text class="_text data-v-17b5b1e1 num">共 {{list.list_item.length}} 个项目</text>
          </view>
          <view class="_view data-v-17b5b1e1 total">
            <image class="_image data-v-17b5b1e1 timer" src="../../static/icon/time.png" mode="aspectFill"></image> {{list.total}} h </view>
        </view>
        <view class="_view data-v-17b5b1e1 project_item" wx:key="itemIndex" key="{{itemIndex}}" wx:for="{{list.list_item}}" wx:for-index="itemIndex" wx:for-item="item">
          <view class="_view data-v-17b5b1e1 card" style=" {{('background-color:' + item.color + ';')}}">
            <image class="_image data-v-17b5b1e1 icon" src="{{'../../static/card_icon/'+item.icon+'.png'}}" mode="scaleToFill"></image>
          </view>
          <view class="_view data-v-17b5b1e1 title">
            <text class="_text data-v-17b5b1e1">{{item.name}}</text>
            <text class="_text data-v-17b5b1e1 second">最近更新：{{item.date}}</text>
          </view>
          <view class="_view data-v-17b5b1e1 time">{{item.time}} h</view>
        </view>
      </view>
    </view>
  </view>
</template>