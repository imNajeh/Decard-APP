<template name="164ad51b">
  <view class="_view data-v-f75b5aa8 me">
    <view class="_view data-v-f75b5aa8 tologin_wrap" wx:if="{{!isLogin}}">
      <text class="_text data-v-f75b5aa8 to_login_text">期待与你完美邂逅～</text>
      <image class="_image data-v-f75b5aa8 logo" src="../../static/logo/192x192-logo.png" mode="scaleToFill"></image>
      <button type="default" plain="{{true}}" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}" class="_button data-v-f75b5aa8">去登录</button>
    </view>
    <view class="_view data-v-f75b5aa8 me_index" wx:else>
      <view class="_view data-v-f75b5aa8 top_intro">
        <image class="_image data-v-f75b5aa8 avatar" src="{{avatar?avatar:'../../static/images/avatar.jpg'}}" mode="scaleToFill" bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}"></image>
        <text class="_text data-v-f75b5aa8 username" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}">{{nickname?nickname:username}}</text>
      </view>
      <view class="_view data-v-f75b5aa8 me_list">
        <view class="_view data-v-f75b5aa8 list_item">我的逗币: {{coin}}</view>
        <view class="_view data-v-f75b5aa8 list_item" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}">我的专注</view>
        <view class="_view data-v-f75b5aa8 list_item" bindtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}">我的发布</view>
        <view class="_view data-v-f75b5aa8 list_item" bindtap="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}">退出登录</view>
      </view>
    </view>
  </view>
</template>